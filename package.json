  "scripts": {
    "webpack:client": "webpack --config webpack.client.js --progress",
    "webpack:server": "webpack --config webpack.server.js --progress",
    "webpack:start": "nodemon --watch build --exec node build/bundle.js",
    "dev": "npm-run-all --parallel webpack:*",
